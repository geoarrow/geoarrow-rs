{
  "scripts": {
    "build": "bash ./scripts/build.sh",
    "build:test": "ENV='DEV' yarn build",
    "build:geoparquet": "FEATURES='--no-default-features --features debug --features parquet' NAME='@geoarrow/geoparquet-wasm' bash ./scripts/build.sh",
    "build:flatgeobuf": "FEATURES='--no-default-features --features debug --features flatgeobuf' NAME='@geoarrow/flatgeobuf-wasm' bash ./scripts/build.sh",
    "docs:build": "typedoc",
    "docs:serve": "cd docs_build && http-server",
    "docs:publish": "gh-pages --dist docs_build --dest js/",
    "test": "ts-node node_modules/tape/bin/tape ./tests/js/index.ts"
  },
  "devDependencies": {
    "@swc/core": "^1.3.68",
    "@swc/helpers": "^0.5.1",
    "@types/node": "^20.4.0",
    "@types/tape": "^5.6.0",
    "apache-arrow": "^12.0.1",
    "arrow-js-ffi": "^0.2.0",
    "gh-pages": "^5.0.0",
    "http-server": "^14.1.1",
    "tape": "^5.6.4",
    "ts-node": "^10.9.1",
    "typedoc": "0.24.8",
    "typescript": "^4.6.2"
  },
  "volta": {
    "node": "20.4.0",
    "yarn": "3.3.1"
  }
}
